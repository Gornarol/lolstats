{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "rules": {
      "no-nested-ternary": 0,
      "no-console": 0,
      "react/prop-types": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 120
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    },
    "plugins": ["react-hooks", "prettier"],
    "parser": "babel-eslint",
    "globals": {
      "FormData": true
    }
  }
  